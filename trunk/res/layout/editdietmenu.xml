		      	<TextView android:text="@string/edit_kaloritext" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5dip"/>
	       		<EditText android:id="@+id/edit_kalori" android:layout_width="fill_parent" android:layout_height="wrap_content" android:width="100dip" android:layout_marginLeft="20px" android:layout_marginRight="20px" android:layout_marginTop="3dip" android:inputType="numberDecimal" android:numeric="decimal"/>
	       		<TextView android:text="@string/edit_kalori_i" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
       		</TableRow>
