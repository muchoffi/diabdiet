	       			<RadioButton android:id="@+id/edit_jenisdiet_b" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/edit_jenisdiet_b"  android:layout_marginRight="5dip" />
	       			<RadioButton android:id="@+id/edit_jenisdiet_b1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/edit_jenisdiet_b1" />
	       		</RadioGroup>
       		</TableRow>
       </TableLayout>		
